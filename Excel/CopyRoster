Sub CopyRoster()

Worksheets("Week 1").Range("A1:H33").Copy _
    Destination:=Worksheets("Printout").Range("A1:H33")

Worksheets("Week 2").Range("A1:H33").Copy _
    Destination:=Worksheets("Printout").Range("A34:H66")
    
Worksheets("Week 3").Range("A1:H33").Copy _
    Destination:=Worksheets("Printout").Range("A67:H99")
    
Worksheets("Week 4").Range("A1:H33").Copy _
    Destination:=Worksheets("Printout").Range("A100:H132")

Worksheets("Printout").Columns("A:H").AutoFit

End Sub
